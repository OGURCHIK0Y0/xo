spisok=[[".",".","."],
        [".",".","."],
        [".",".","."]]


while (True):
    x=int(input("введите х для 1 игрока"))
    y=int(input("введите y для 1 игрока"))
    spisok[x][y]="X"
    x1=int(input("введите х для 2 игрока"))
    y1=int(input("введите y для 2 игрока"))
    spisok[x1][y1]="O"
    print(spisok)
